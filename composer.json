{
	"name"         : "busyphp/framework",
	"type"         : "project",
	"description"  : "",
	"authors"      : [
		{
			"name"  : "busy^life",
			"email" : "busy.life@qq.com"
		}
	],
	"license"      : "Apache-2.0",
	"require"      : {
		"php"             : ">=8.0",
		"busyphp/busyphp" : "^7.0"
	},
	"require-dev"  : {
		"symfony/var-dumper" : "^4.2",
		"phpunit/phpunit"    : "^9.0"
	},
	"autoload"     : {
		"psr-4"                 : {
			"app\\"  : "app",
			"core\\" : "core"
		},
		"psr-0"                 : {
			"" : ["extend/", "vendor/busyphp/busyphp/assets"]
		},
		"exclude-from-classmap" : [
			"runtime"
		]
	},
	"autoload-dev" : {
		"psr-4" : {
			"tests\\" : "tests/"
		}
	},
	"config"       : {
		"preferred-install" : "dist"
	},
	"scripts"      : {
		"post-autoload-dump" : [
			"@php think service:discover",
			"@php think vendor:publish",
			"@php think bp:publish"
		]
	}
}
