{
	"name"        : "busyphp/framework",
	"type"        : "project",
	"description" : "",
	"authors"     : [
		{
			"name"  : "busy^life",
			"email" : "busy.life@qq.com"
		}
	],
	"license"     : "CC-BY-NC-4.0",
	"require"     : {
		"php"             : ">=7.1.0",
		"busyphp/busyphp" : "^6.0"
	},
	"require-dev" : {
		"symfony/var-dumper" : "^4.2",
		"busyphp/install"    : "^1.0"
	},
	"autoload"    : {
		"psr-4"                 : {
			"app\\"                         : "app",
			"core\\"                        : "core",
			"BusyPHP\\app\\admin\\assets\\" : "vendor/busyphp/busyphp/src/app/admin/static"
		},
		"psr-0"                 : {
			"" : "extend/"
		},
		"exclude-from-classmap" : [
			"runtime"
		]
	},
	"config"      : {
		"preferred-install" : "dist"
	},
	"scripts"     : {
		"post-autoload-dump" : [
			"@php think service:discover",
			"@php think vendor:publish",
			"@php think bp:install"
		]
	}
}
